﻿body {
	font-family: Arial;
	font-size: 12px;
	color: #565766;
}
span.lienVideo,
span.LienVideo
 {
  background:url('../Images/pictoCam_off.gif') left 2px no-repeat;
  padding-left:20px;
}
span.lienArticle,
span.LienArticle
 {
  background:url('../Images/formationInitiale/pictoArticle.gif') left top no-repeat;
  padding-left:15px;
  display:block;
  height:20px;
}

h1, h2, h3, h4 {
	margin-top:4px;
	margin-bottom:4px;
}
h1, h2, h3 {
	margin-bottom:6px;
}
h1,
h1 p
{
    color:#000000;
    font-size:16px;
    font-weight:bold;
    border-bottom: solid 1px #000000;
    padding-bottom:12px;
    padding-left:40px;
}
h2,
h2 p
{
    color:#000000;
    font-size:14px;
    font-weight:bold;
}
h3 
{
    color:#39A612;
}
h4 
{
    color:#800695;
    color:#006B66;
}
h5 
{
    color:#12247C;
}

p{
	font-family: Arial;
	font-size: 12px;
	color: #565766;
	line-height: 1.4em;
	text-align:justify;
    margin-top:3px;
 	margin-bottom:5px;
}
p.note 
{
	font-size:10px;
}
img 
{
    border-width:0px;
}
.gras 
{
    font-weight:bold;
}
.texteImportant
{
	font-size: 11pt;
	color: #828599;
	font-weight:bold;
	padding-top:10px;
}
hr.retour
{
     clear:both;
     visibility:hidden !important;
     border:0;
     height:0px;
}

ul li,
ol li
{
	text-align:justify;
}



span.lienAncreArticle a:link,
span.lienAncreArticle a:active,
span.lienAncreArticle a:visited,
span.lienAncreArticle a:hover
{
	background:url(../Images/Articles/fleche_bas.gif) no-repeat left 5px;
	color:#eb0009;
	display:inline-block;
	padding-left:20px;
	height:20px;
}


/***************/
/*   Cadre Large     */
/***************/
.cadreLarge 
{
    margin:auto;
    width:607px;
}
.cadreLarge .topCadre
{
    margin-top:20px;
    width:607px;
    height:22px;
    background:url('../Images/aucun/topCadreLarge.gif') left bottom no-repeat;
}
.cadreLarge .bottomCadre
{
    width:607px;
    height:16px;
    background:url('../Images/aucun/bottomCadreLarge.gif') left top no-repeat;
    margin-bottom:15px;
}
.cadreLarge .contenuCadre
{
    padding:0 15px;
    width:575px;
    background-color:White;
    border:solid 1px #626371;
    border-width:0 1px;
}
.cadreLarge .contenuCadre h2 
{
    margin:0;
}


/***************/
/*   Cadre Large     */
/***************/
.cadre500
{
    margin:auto;
    width:507px;
}
.cadre500 .topCadre
{
    margin-top:20px;
    width:507px;
    height:15px;
    background:url('../Images/espacePerso/topCadreMedium.gif') left top no-repeat;
}
.cadre500 .bottomCadre
{
    width:507px;
    height:16px;
    background:url('../Images/espacePerso/bottomCadreMedium.gif') left top no-repeat;
    margin-bottom:15px;
}
.cadre500 .contenuCadre
{
    padding:0 15px;
    width:468px;
    background-color:White;
    border:solid 1px #626371;
    border-width:0 1px;
}
.cadre500 .contenuCadre h2 
{
    margin:0;
